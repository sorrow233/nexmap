{
  "name": "mixboard-canvas",
  "private": false,
  "version": "2.2.27",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/generate-sitemap.js && npx vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy:main": "npm run build && npx wrangler pages deploy dist --project-name aimainmap --branch main",
    "deploy:beta": "npm run build && npx wrangler pages deploy dist --project-name aimainmap --branch beta",
    "deploy:alpha": "npm run build && npx wrangler pages deploy dist --project-name aimainmap --branch alpha",
    "ship:main": "npm run deploy:main && git push origin main",
    "ship:beta": "npm run deploy:beta && git push origin beta",
    "ship:alpha": "npm run deploy:alpha && git push origin alpha",
    "deploy": "npm run deploy:beta",
    "ship": "npm run ship:beta"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@use-gesture/react": "^10.3.1",
    "dompurify": "^3.3.1",
    "firebase": "^10.7.1",
    "highlight.js": "^11.11.1",
    "html-to-image": "^1.11.13",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.294.0",
    "marked": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hotkeys-hook": "^5.2.1",
    "react-router-dom": "^7.11.0",
    "use-sync-external-store": "^1.6.0",
    "zundo": "^2.3.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^5.0.0"
  }
}